{
  "name": "pono-ono",
  "version": "0.0.0",
  "description": "wordpress theme built with timber + biggie",
  "homepage": "https://github.com/mikehwagz/pono-ono",
  "repository": {
    "type": "git",
    "url": "https://github.com/mikehwagz/pono-ono.git"
  },
  "scripts": {
    "lint": "standard --verbose | snazzy"
  },
  "author": "mike wagz",
  "main": "gulpfile.js",
  "browser": {
    "framework": "./src/js/framework",
    "env": "./src/js/env",
    "config": "./src/js/config",
    "cache": "./src/js/cache",
    "utils": "./src/js/utils",
    "@utils/array": "./src/js/utils/array",
    "@utils/biggie": "./src/js/utils/biggie",
    "@utils/css": "./src/js/utils/css",
    "@utils/dom": "./src/js/utils/dom",
    "@utils/func": "./src/js/utils/func",
    "@utils/math": "./src/js/utils/math"
  },
  "dependencies": {
    "bigwheel": "3.0.0",
    "bourbon": "^4.3.3",
    "bw-router": "^2.0.6",
    "bw-viewmediator": "2.1.0",
    "bw-vm": "^2.1.3",
    "dom-classes": "^1.0.0",
    "dom-create-element": "^1.0.2",
    "dom-event": "^1.0.0",
    "dom-select": "^1.0.0",
    "gsap": "^1.19.0",
    "hammerjs": "^2.0.8",
    "jquery": "^3.2.1",
    "pixi.js": "4.5.1",
    "please-ajax": "^2.0.2",
    "prefix": "^1.0.0",
    "query-dom-components": "^0.1.0",
    "slider-manager": "^2.0.1",
    "smooth-scrolling": "^2.3.7",
    "sniffer": "github:watsondg/sniffer",
    "virtual-scroll": "^1.1.1"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.22.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.14.3",
    "browserify": "^14.0.0",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-changed": "^1.3.2",
    "gulp-cssnano": "^2.1.2",
    "gulp-html-replace": "^1.6.2",
    "gulp-imagemin": "^3.0.3",
    "gulp-load-plugins": "^1.2.4",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.3.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.7",
    "normalize.css": "^4.2.0",
    "require-dir": "^0.3.0",
    "standard": "^10.0.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "only": "src/javascript/",
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "standard": {
    "ignore": [
      "/build/",
      "/dist/",
      "/gulp/",
      "gulpconfig.js",
      "gulpfile.js"
    ],
    "globals": [
      "TimelineLite",
      "TimelineMax",
      "TweenLite",
      "TweenMax",
      "Back",
      "Bounce",
      "Circ",
      "Cubic",
      "Ease",
      "EaseLookup",
      "Elastic",
      "Expo",
      "Linear",
      "Power0",
      "Power1",
      "Power2",
      "Power3",
      "Power4",
      "Quad",
      "Quart",
      "Quint",
      "RoughEase",
      "Sine",
      "SlowMo",
      "SteppedEase",
      "Strong",
      "Draggable",
      "SplitText",
      "VelocityTracker",
      "CSSPlugin",
      "ThrowPropsPlugin",
      "BezierPlugin",
      "APP",
      "PIXI"
    ]
  }
}
